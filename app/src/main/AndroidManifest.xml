<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cloudsherpas.kandidatos" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".candidate.CandidateListActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".profile.CandidateProfileActivity"
            android:label="@string/title_activity_candidate_profile" >
        </activity>
        <activity
            android:name=".bio.BiographyActivity"
            android:label="@string/title_activity_biography"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>
